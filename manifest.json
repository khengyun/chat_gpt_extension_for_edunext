{
  "name": "Gờ Bờ Tờ For Edunext",
  "description": "Gờ Bờ Tờ For Edunext from HOVILO \nby Khengyun",
  "default_locale": "en",
  "version": "1.16.3",
  "manifest_version": 3,
  "icons": {
    "16": "logo.png",
    "32": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  },
  "host_permissions": ["https://*.openai.com/"],
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://fu.edunext.vn/*/session/activity?sessionid=*&activityId=*"
      ],
      "js": ["content-script.js"],
      "css": ["content-script.css"]
    }
  ]
}
